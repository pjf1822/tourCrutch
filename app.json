{
  "expo": {
    "name": "Tour Crutch",
    "slug": "tourCrutch",
    "version": "1.2.29",
    "orientation": "portrait",
    "icon": "./assets/nailed-it.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DJ.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to choose youre profile pic."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pjf1822.tourCrutch",
      "buildNumber": "29",
      "icon": "./assets/nailed-it.jpg",  
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleIconName": "logito",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "I am not using any user location data nor do i have access to it",
        "NSLocationWhenInUseUsageDescription":"I am not using any user location data nor do i have access to it",
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": ["DDA9.1", "C617.1", "3B52.1", "0A2A.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": ["35F9.1", "8FFB.1", "3D61.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": ["85F4.1", "E174.1", "7D9E.1", "B728.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1", "1C8F.1", "C56D.1", "AC6B.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/nailed-it.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "63cf35c1-a0a8-496e-a72e-daa2ebe06522"
      }
    }
